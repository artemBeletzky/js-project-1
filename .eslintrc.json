{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
    //        "plugin:jest/all"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }],
    "comma-dangle": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "import/extensions": 0
  }
  //    "plugins": [
  //        "jest"
  //    ]
}
